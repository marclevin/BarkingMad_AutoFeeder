{
    "manifest_version": 3,
    "name": "BarkingMad Autofeeder",
    "description": "Automatically visit BarkingMad.co.za and click the 'Feed' button, 5 times daily. Feeding dogs on your behalf!",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
        "https://www.barkingmad.co.za/*",
        "https://www.barkingmad.co.za/click-to-feed-2019-2/*",
        "https://barkingmad.co.za/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/dog16.png",
        "32": "images/dog32.png",
        "128": "images/dog128.png"
      }
    }
  }
  